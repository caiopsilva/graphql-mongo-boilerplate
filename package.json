    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.4.0",
    "kcors": "^2.2.2",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-graphql": "^0.8.0",
    "koa-graphql-batch": "^1.1.0",
